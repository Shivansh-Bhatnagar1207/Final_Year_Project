{
  "expo": {
    "name": "FITNESS",
    "slug": "FITNESS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon-fitness.png",
    "scheme": "fitness.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.shivansh_bhatnagar1207.FITNESS"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion"
        }
      ],

      ["expo-notifications"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-fitness.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "08df44dc-21dd-422d-8cef-607de9b88985"
      }
    }
  }
}
